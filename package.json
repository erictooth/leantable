{
	"name": "leantable",
	"version": "0.3.2",
	"description": "A fast, extensible table component",
	"keywords": [
		"table",
		"grid",
		"react",
		"component"
	],
	"author": "erictooth",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/erictooth/leantable.git"
	},
	"homepage": "https://github.com/erictooth/leantable",
	"bugs": {
		"url": "https://github.com/erictooth/leantable/issues"
	},
	"files": [
		"dist-css",
		"dist-cjs",
		"dist-esm"
	],
	"exports": {
		"./core": {
			"import": "./dist-esm/core/index.js",
			"require": "./dist-cjs/core/index.js"
		},
		"./css/*": "./dist-css/*",
		"./plugins": {
			"import": "./dist-esm/plugins/index.js",
			"require": "./dist-cjs/plugins/index.js"
		},
		"./react": {
			"import": "./dist-esm/react/index.js",
			"require": "./dist-cjs/react/index.js"
		}
	},
	"packageManager": "pnpm@7.12.2",
	"devDependencies": {
		"@erictooth/eslint-config": "^8",
		"@erictooth/semantic-release-npm-github-config": "^1.0.3",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.3",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.21",
		"chokidar-cli": "^3",
		"eslint": "^8",
		"glob": "^8",
		"prettier": "~2.7.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"semantic-release": "17",
		"typescript": "~4.7"
	},
	"scripts": {
		"prepack": "make prepack"
	},
	"peerDependencies": {
		"react": ">= 16.8.0"
	},
	"dependencies": {
		"clsx": "^1.0.0",
		"rxjs": "^7.0.0"
	},
	"release": {
		"extends": "@erictooth/semantic-release-npm-github-config",
		"branches": [
			"main"
		]
	}
}
